<%= page_title 'Resend unlock instructions' %>

<%= pretty_form_for(resource, :as => resource_name, :url => unlock_path(resource_name), :html => { :method => :post }) do |f| %>
  <%= devise_error_messages! %>
  <%= f.item(:email){ f.email_field :email } %>
  <%= f.submit_area 'Resend Instructions' %>
<% end %>

<%= render "devise/shared/links" %>
